{
  "name": "recall",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky",
    "hkr:pg:parser:generate": "turbo run hkr:pg:parser:generate",
    "clean:all": "git clean -fdX",
    "clean:dist": "git clean -fdX packages/*/dist",
    "clean:node_modules": "git clean -fdX packages/*/node_modules"
  },
  "devDependencies": {
    "husky": "^9.0.11",
    "prettier": "^3.3.2",
    "turbo": "^2.0.4",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  }
}
